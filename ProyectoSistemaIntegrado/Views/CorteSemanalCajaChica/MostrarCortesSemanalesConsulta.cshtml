@using Microsoft.AspNetCore.Http;
@{
    Context.Session.SetString("tituloPrincipal", "Consulta de Cortes de Caja Chica");
}

<fieldset>
    <form method="post" id="frmBusquedaCortesSemanales" name="frmBusqueda">
        <div style="margin-left: auto; margin-right: auto; text-align: center; width: 100%">
            <table>
                <tr>
                    <td>CAJA CHICA</td>
                    <td>AÑO</td>
                    <td></td>
                </tr>
                <tr>
                    <td>
                        <select class="form-control" id="uiFiltroCajaChica" name="CodigoCajaChica">
                        </select>
                    </td>
                    <td>
                        <input type="text" name="AnioOperacion" class="form-control solonumeros-opcional text-center" id="uiFiltroAnioOperacion" placeholder="Año" />
                    </td>
                    <td>
                        <input type="button" class="form-control btn btn-primary" value="BUSCAR" onclick="buscarCortesSemanales()" />
                    </td>
                </tr>
            </table>
        </div>
    </form>
</fieldset>

<div style="height: 15px; width:100%;">
</div>

<div id="divTabla">
</div>

@section Scripts {
    <script src="~/js/datatable.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/jqueryui.js"></script>
    <script src="~/js/sweetalert.js"></script>
    <script src="~/js/generic.js"></script>
    <script src="~/js/cortesemanalcajachica.js"></script>
}