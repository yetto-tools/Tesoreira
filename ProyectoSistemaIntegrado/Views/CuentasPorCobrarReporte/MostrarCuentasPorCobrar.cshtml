@using Microsoft.AspNetCore.Http;
@using Newtonsoft.Json;
@{
    ViewData["Title"] = "Create";
    Context.Session.SetString("tituloPrincipal", "Reportes Semanales de Cuentas Por Cobrar (Consulta)");
}

<div style="height: 25px; width:100%;"></div>
<a href="@Url.Content("~")" id="uiExportarExcel">Exportar Excel</a>

<div style="height: 25px; width:100%;">
</div>
<table style="margin-left: auto; margin-right: auto;">
    <tr>
        <td>AÑO</td>
        <td>SEMANA</td>
        <td></td>
    </tr>
    <tr>
        <td>
            <select class="form-control obligatorio" id="uiAnioReporte" name="AnioOperacion" onchange="fillSemanasReporteCuentasPorCobrar()">
            </select>
        </td>
        <td>
            <select class="form-control obligatorio" id="uiSemanaReporte" name="NumeroSemana">
            </select>
        </td>
        <td>
            <button class="btn btn-success" onclick="ListarCuentasPorCobrarReporteConsulta()">BUSCAR</button>
        </td>
    </tr>
</table>
  
<div style="height: 25px; width: 100%;">
</div>

<div id="divTabla">
</div>


@section Scripts {
    <script src="~/js/datatable.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/jqueryui.js"></script>
    <script src="~/js/sweetalert.js"></script>
    <script src="~/js/generic.js"></script>
    <script src="~/js/cxc.js"></script>
}