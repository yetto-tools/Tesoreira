@using Microsoft.AspNetCore.Http;
@{
    Context.Session.SetString("tituloPrincipal", "Generación de Traslados Especiales 2");
}

<button type="button" class="btn btn-outline-success" onclick="Redireccionar('Reportes', 'Index')">
    <i class="bi bi-arrow-left-circle">
        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-arrow-left-circle" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8zm15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.5-.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5z" />
        </svg>
    </i>
    Regresar
</button>

<div style="height: 15px; width:100%;">

</div>

<form method="post" id="frmBusqueda">
    <table style="margin-left: auto; margin-right: auto;">
        <tr>
            <td>FECHA OPERACION</td>
            <td></td>
        </tr>
        <tr>
            <td>
                <input type="text" name="FechaOperacion" class="form-control text-center obligatorio" id="uiFechaOperacion" placeholder="Fecha Operación" />
            </td>
            <td>
                <input type="button" class="form-control btn btn-primary" value="GENERAR" onclick="GenerarTrasladosEspeciales2()" />
            </td>
        </tr>
    </table>
</form>

<div style="height: 15px; width:100%;">

</div>

<div id="divTabla">

</div>

@section Scripts {
    <script src="~/js/datatable.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/jqueryui.js"></script>
    <script src="~/js/sweetalert.js"></script>
    <script src="~/js/generic.js"></script>
    <script src="~/js/crom.js"></script>
    <script>
        $(function () {
            $("#uiFechaOperacion").datepicker({
                dateFormat: 'dd/mm/yy',
                changeYear: true
            });
        });
    </script>

}